    chmod 664 /sys/class/input_booster/multitouch/tail

on post-fs-data
    mkdir /dev/socket/stork 0770 system system

    # USB conn_gadget device (DeXonPC, Samsung Flow)
    chown system system /dev/android_ssusbcon
    chmod 0660 /dev/android_ssusbcon

    chown system radio /efs
    chmod 0771 /efs
    restorecon_recursive /efs

    # sec_efs_file
    mkdir /efs/sec_efs 0775 radio system

    # smart switch mobile
    mkdir /efs/ssm 0770 system system

on boot
    # Tweak background writeout
    write /proc/sys/vm/dirty_expire_centisecs 200

    # VM dirty thresh setting
    write /proc/sys/vm/dirty_bytes 104857600
    write /proc/sys/vm/dirty_background_bytes 26214400

    # Memory management.  Basic kernel parameters, and allow the high
    # level system server to be able to adjust the kernel OOM driver
    # parameters to match how it is managing things.
    write /proc/sys/vm/watermark_boost_factor 0

    # System server manages zram writeback
    chown root system /sys/block/zram0/writeback_limit
    chmod 0664 /sys/block/zram0/writeback_limit
    chown root system /sys/block/zram0/bd_stat
    chmod 0664 /sys/block/zram0/bd_stat
