    chmod 0664 /sys/block/zram0/disksize
    chown root system /dev/block/zram0
    chmod 0660 /dev/block/zram0

    # am_app_launch
    chmod 0664 /sys/kernel/mm/vmscan/am_app_launch
    chown system system /sys/kernel/mm/vmscan/am_app_launch

